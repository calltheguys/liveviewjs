{
  "name": "liveviewjs",
  "version": "0.0.1",
  "description": "LiveViewJS brings the power of Phoenix LiveView to Typescript and Javascript developers and applications.",
  "targets": {
    "client": {
      "source": "src/client/liveview.ts",
      "context": "browser",
      "distDir": "dist/client"
    },
    "framework": {
      "source": "src/server/index.ts",
      "context": "node",
      "isLibrary": true,
      "distDir": "dist/framework"
    },
    "examples": {
      "source": "src/examples/index.ts",
      "context": "node",
      "distDir": "dist/examples"
    }
  },
  "source": "src/server/index.ts",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "examples": "nodemon -e js -w dist dist/examples/index.js",
    "preexamples": "npm run build",
    "build": "rm -rf dist; npm run check; parcel build",
    "watch": "parcel watch",
    "check": "tsc --noEmit",
    "test": "jest --expand"
  },
  "keywords": ["liveviewjs","liveview", "phoenix", "typescript", "javascript", "framework"],
  "author": "Donnie Flood <donnie@floodfx.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.2.0",
    "nprogress": "^0.2.0",
    "phoenix": "^1.6.6",
    "phoenix_html": "^3.2.0",
    "phoenix_live_view": "^0.17.6",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.0",
    "@types/ws": "^8.2.2",
    "eslint": "^8.5.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "parcel": "^2.2.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
